/*--通用基础样式 开始--*/
@charset "UTF-8";
body{margin:0; padding:0; font-size:14px; font-family:'Hiragino Sans GB',Arial, Helvetica, sans-serif; -webkit-font-smoothing: antialiased; -moz-font-smoothing: antialiased; -ms-font-smoothing: antialiased; font-smoothing: antialiased; color:#666;}
body *{-webkit-box-sizing:border-box; -moz-box-sizing:border-box;box-sizing: border-box;}
h1,h2,h3,h4,h5,h6,ul,li,dl,dt,dd,p,input{margin:0; padding:0;}
a:link,a:visited{text-decoration:none; color:#333;}
a:hover{text-decoration:underline; color:#FF0000;}
img{border:0;}
.imghover{-webkit-transition: all 0.5s ease 0s; -moz-transition: all 0.5s ease 0s; transition: all 0.5s ease 0s;}
.imghover:hover{-webkit-transform: scale(1.1); -moz-transform: scale(1.1); transform: scale(1.1);}
input,select,button,textarea{outline:none;}
input[type="submit"],input[type="text"],button{-webkit-appearance:none;}
.imghover{-webkit-transition: all 0.5s ease 0s; -moz-transition: all 0.5s ease 0s; transition: all 0.5s ease 0s;}
.imghover:hover{-webkit-transform: scale(1.1); -moz-transform: scale(1.1); transform: scale(1.1);}
li{list-style:none;}
i,em{font-style:normal;}
.fl{float:left;}
.fr{float:right;}
.clear{clear:both;}
.height10{height:10px;}
.height20{height:20px;}
.height30{height:30px;}
/*--通用基础样式 结束--*/
@font-face {font-family: 'iconfont';
    src: url('../fonts/iconfont.eot'); /* IE9*/
    src: url('../fonts/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('../fonts/iconfont.woff') format('woff'), /* chrome、firefox */
    url('../fonts/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
    url('../fonts/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}
.iconfont{
    font-family:"iconfont" !important;
    font-size:16px; font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
}
.icon {
   width: 1em; height: 1em;
   vertical-align: -0.15em;
   fill: currentColor;
   overflow: hidden;
}

