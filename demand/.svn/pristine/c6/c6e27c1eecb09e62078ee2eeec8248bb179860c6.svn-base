}
 .pic_abox-c{position: absolute; z-index: 9; right:524px; bottom:-45px; animation: pic_abox-cimg 20s linear infinite alternate;}
  @keyframes pic_abox-cimg{
  0%{transform: translateY(50px) translateX(50px) scale(1) rotate(0deg);}
  25%{transform:rotate(5deg);}
