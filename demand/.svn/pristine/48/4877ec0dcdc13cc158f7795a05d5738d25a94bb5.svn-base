/*弹窗登录*/
.loginFormCover{
	display: none;
	position: absolute;
	width: 100%;
	height: 100%;
	z-index: 999998;
	background-color: rgba(0,0,0,0.4);
	left: 0;
	top: 0;
}
.loginForm{
	display: none;
	position: fixed;
	z-index: 999999;
	width: 700px;
	left: 50%;
	margin-left: -350px;
	background-color: #fff;
	top:50%;
	margin-top: -250px;
}
.loginForm>i{
    display: block;
    clear: both;
    position: absolute;
    color: #fff;
    font-size: 40px;
    right: -33px;
    top: -16px;
    cursor: pointer;
}
.loginForm li{
	margin-bottom: 16px;
	position: relative;
}
.loginForm li span{
	color: #fa5d5d;
	position: absolute;
	bottom: -12px;
	left: 0;
}
.loginForm li span i{
	position: relative;
	top: 1px;
}
.loginForm .loginFormBox{
	width: 300px;
	margin: 0 auto;
	padding: 20px 0;
}

.loginForm .loginFormBox .loginFormTab a{
	display: inline-block;
	font-size: 16px;
	color: #666;
	padding: 10px 20px;
	cursor: pointer;
	margin-bottom: 20px;
}
.loginForm .loginFormBox .loginFormTab a.aft{
	/*border-bottom:4px solid #479ef8 ;*/
	color: #479ef8;
	position: relative;
}
.loginForm .loginFormBox .loginFormTab a.aft:after{
	content: '';
	display: inline-block;
	position: absolute;
	left: 0;
	bottom: 0;
	background-color: #479ef8;
	height: 3px;
	width: 100%;
}
.loginForm .loginFormBox .loginFormTab a+a{
	float: right;
}
.loginForm .reminder{
	border: 1px solid #fdb053;
	margin: 20px 0;
	background-color: #fff6dc;
	padding: 4px 0;
	text-align: center;
	font-size: 12px;
	color: #666;
	box-sizing: border-box;
}
.loginForm .reminder i{
	font-size: 20px;
	color: #fdb053;
	position: relative;top: 2px;
}
.loginForm .inputTextBD{
	width: 100%;
	margin-bottom: 10px;
	height: 40px;
	line-height: 40px;
	border-radius: 4px;
	font-size: 12px;
}
.loginForm .imgCodeBox .inputTextBD{
	width: 190px;
	float: left;
	margin-right: 10px;
}
.loginForm .imgCodeBox .inputTextBD input{
	width: 140px;
