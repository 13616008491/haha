	margin-top: 20px;
}
.mainLoginTab a{
	color: #eee;
	display: inline-block;
	height: 40px;
	padding: 0 10px;
	float: left;
	font-size: 12px;
	line-height: 40px;
	cursor: pointer;
	transition: all .5s;
	-moz-transition: all .5s;	/* Firefox 4 */
	-webkit-transition: all .5s;	/* Safari 和 Chrome */
	-o-transition: all .5s;	/* Opera */
}
.mainLoginTab a.aft{
	color: #479ef8;
}
.mainLoginTab i{
	width: 68px;
	height: 2px;
	position: absolute;
	bottom: 0px;
	left: 40px;
	background-color: #479ef8;
	transition: all .5s;
	-moz-transition: all .5s;	/* Firefox 4 */
	-webkit-transition: all .5s;	/* Safari 和 Chrome */
	-o-transition: all .5s;	/* Opera */
}
.mainLoginTab a+a{
	float: right;
}
.mainLoginForm{
	margin-top: 20px;
}
.mainLoginForm p{
	width: 100%;
	border: 1px solid #979797;
	height: 50px;
	border-radius: 4px 4px 0 0;
	line-height: 50px;
	position: relative;
	padding-left: 20px;
}
.mainLoginForm p+p{
	border-top:0;
	border-radius: 0;
}
.mainLoginForm p+p+p{
	border-top:0;
	border-radius: 0 0 4px 4px;
}

.mainLoginForm p label{
	color: #fff;
	position: absolute;
	left: 20px;
	height: 50px;
	line-height: 50px;
}
.mainLoginForm p input{
	color: #fff;
	border: none ;
}
.mainLoginForm p i{
	position: absolute;
    display: inline-block;
    height: 40px;
    width: 120px;
    right: 0;
    top: 5px;
    cursor: pointer;
}
.mainLoginForm p i img{
	height: 40px;
    width: 100px;
}
.mainLoginForm p b{
	display: inline-block;
	border: 1px solid #479ef8;
	color: #479ef8;
	height: 30px;
	line-height: 30px;
	padding: 0 10px;
	border-radius:4px ;
	cursor: pointer;
	position: absolute;
	right: 10px;
	top: 10px;
}
.mainLoginMenu{
	height: 40px;
