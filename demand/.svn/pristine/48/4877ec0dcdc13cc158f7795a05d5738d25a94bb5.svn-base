
@import "global.css";
/*
 *右边悬浮菜单
 * */
.hide{
	display: none;
}
.show{
	display:block ;
}

.toolbar{
	position: fixed;
	right: 20px;
	bottom: 120px;
	z-index: 1990;
}
.toolbar li{
	width: 50px;
	height: 50px;
	background: #fff;
	text-align: center;
	line-height: 50px;
	z-index: 999999999;
	margin-bottom: 4px;
	cursor: pointer;
	position: relative;
	border: 1px solid #eee;
	border-radius: 4px;
}
.toolbar li:first-child{
	margin-bottom: 20px;
}
.toolbar li:first-child .con-roll i{
	color: #fff;
	background-color: #479ef8;
	border-radius: 4px;
}

.toolbar li .con-box{
	line-height: 20px;
	position: absolute;
	width: 240px;
	background: #fff;
	left: -246px;
	visibility: hidden;
	opacity: 0;
	z-index: 999999999;
	border: 1px solid #eee;
	transition: visibility 0.5s,opacity 0.5s;
	-moz-transition: visibility 0.5s,opacity 0.5s; /* Firefox 4 */
	-webkit-transition: visibility 0.5s,opacity 0.5s; /* Safari 和 Chrome */
	-o-transition: visibility 0.5s,opacity 0.5s; /* Opera */
	cursor: default;
}
.toolbar li .con-roll{
	height: 50px;
	width: 50px;
	overflow: hidden;
}
.toolbar li .con-roll i{
	display: block;
	width: 50px;
	height: 50px;
	margin-top: 0px;
	font-size: 28px;
	transition: margin-top 0.5s;
	-moz-transition: margin-top 0.5s; /* Firefox 4 */
	-webkit-transition: margin-top 0.5s; /* Safari 和 Chrome */
	-o-transition: margin-top 0.5s; /* Opera */
	color: #aeaeae;
}
.toolbar li p{
    position: relative;
    top: 3px;
    left: -1px;
    height: 49px;
    width: 49px;
    padding: 14px 10px;
    font-size: 12px;
    line-height: 14px;
    background-color: #479ef8;
    color: #fff;
    border-radius: 4px;
}
.toolbar>ul>li:hover .con-box{
	/*left: -306px;*/
	opacity: 1;
	visibility:initial;
}
.toolbar>ul>li:hover .con-roll i{
	margin-top: -54px;
}
.toolbar>ul>li:first-child .con-roll i{
	width: 48px;
	height: 48px;
	margin-top: 0px !important;
	border-radius: 4px;
}
.toolbar .telephone .con-box{
	text-align: left;
	padding: 20px 10px 20px 70px;
	position: absolute;
	top: -1px;
}
.toolbar .telephone .con-box i{
	position: absolute;
	left: 20px;
	font-size: 38px;
	color: #479ef8;
	top: 32px;
}
.toolbar .telephone dd{
	font-size: 22px;
	color: #479ef8;
}
.toolbar .app_down li{
	float: left;
	width: 100px;
	height: 100px;
	border: 1px solid #eee;
	padding: 2px;
	text-align: center;
	position: relative;
	margin-right: 20px;
}
.toolbar .app_down .con-box{
	top: -1px;
	padding: 20px;
}
.toolbar .app_down .con-box .imgCenterBox{
	height: auto;
	width: 100%;
}
.toolbar .app_down .con-box .imgCenterBox img{
	width: 120px;
}
.toolbar .app_down li img{
	display: block;
}
.toolbar .app_down li span{
	width: 100%;
	display: block;
	position: absolute;
	bottom: -40px;
	text-align: center;
}
.toolbar .opinion .con-box{
	padding: 20px;
	top: -1px;
}
.toolbar .opinion .con-box textarea{
	width: 100%;
	border: 1px solid #eee;
	resize: none;
	color: #666;
	border-radius: 4px;
	padding: 10px;
}
.toolbar .opinion .con-box textarea::-webkit-input-placeholder{
	color: #999;
}
.toolbar .opinion .con-box textarea::-moz-placeholder{   /* Mozilla Firefox 19+ */
	color: #999;
}
.toolbar .opinion .con-box textarea:-moz-placeholder{    /* Mozilla Firefox 4 to 18 */
	color: #999;
}
.toolbar .opinion .con-box textarea:-ms-input-placeholder{  /* Internet Explorer 10-11 */ 
	color: #999;
}
.toolbar .opinion .con-box .inputTextBD{
	width: 100%;
	height: 40px;
	line-height: 40px;
	border: 1px solid #eee;
	resize: none;
	border-radius: 4px;
	padding:0 10px;
	text-align: left;
}
.toolbar .opinion .con-box .chhBtn{
	width: 100%;
	height: 40px;
	line-height: 40px;
	border-radius: 4px;
	color: #fff;
	background-color: #479ef8;
	margin-top: 10px;
}
.toolbar .return-top {
	visibility: hidden;
	opacity:0;
	transition: visibility 0.5s,opacity 0.5s;
	-moz-transition: visibility 0.5s,opacity 0.5s; /* Firefox 4 */
	-webkit-transition: visibility 0.5s,opacity 0.5s; /* Safari 和 Chrome */
	-o-transition: visibility 0.5s,opacity 0.5s; /* Opera */
}
