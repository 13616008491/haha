}
.loginForm .inputTextBD input::-moz-placeholder{   /* Mozilla Firefox 19+ */
	font-size: 12px;
}
.loginForm .inputTextBD input:-moz-placeholder{    /* Mozilla Firefox 4 to 18 */
	font-size: 12px;
}
.loginForm .inputTextBD input:-ms-input-placeholder{  /* Internet Explorer 10-11 */ 
