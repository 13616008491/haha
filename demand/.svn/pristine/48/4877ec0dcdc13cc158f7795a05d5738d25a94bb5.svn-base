.nav {
	height: 36px;
	margin: 0 auto
}

.nav li {
	float: left
}

.nav a:link,.nav a:visited {
	text-decoration: none;
	line-height: 36px;
	float: left;
	font-size: 18px;
	margin: 0 0 0 70px;
	color: #323232;
	position: relative;
}

.nav a:hover:after,.navbg .nav .active:after {
	opacity: 1;
	display: inline;
}
.navbg .nav .active {
	color: #2692E9;
}

.nav a:after {
	width: 20px;
	height: 2px;
	background-color: #2692E9;
	position: absolute;
	z-index: 1;
	left: 50%;
	margin-left: -10px;
	top: 36px;
	content: "";
	opacity: 0;
	display: none;
	-webkit-transition: all .5s ease 0s;
	-moz-transition: all .5s ease 0s;
	transition: all .5s ease 0s
}
.nav-app-down{
	width: 140px !important;
