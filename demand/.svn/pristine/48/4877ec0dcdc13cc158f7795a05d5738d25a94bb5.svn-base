	margin-right: 10px;
	margin-top: 5px;
}
.loginForm .imgCodeBox .imgCode{
	width: 100px;
    height: 40px;
    float: left;
    border: 1px solid #eee;
    cursor: pointer;
}
.loginForm .imgCodeBox .imgCode img{
	width: 100%;
	height: 100%;
}
