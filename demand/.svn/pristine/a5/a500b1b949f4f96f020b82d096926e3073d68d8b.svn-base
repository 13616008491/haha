.messbut span,.messbut input{ margin:5px 0; display:block; margin-left:10px;}
.messphoto{ padding:0 10px; overflow:hidden;}
.messphoto img{ width:40px; height:40px; display:block;} 

.me_more{ width:100%; text-align:center; background:#f9f9f9; line-height:26px; border-bottom:1px solid #eee;}


/*角色*/
.role{ line-height:200%; overflow:hidden; clear:both; margin:0 0 10px ;}
.ro_d{}
.role label{ color:#555; margin-left:10px; padding:7px 0 0;}
.role label input { margin:0 5px 0 0; padding:0; vertical-align: middle;}
.attr_list li{ float:left; white-space:nowrap; width:200px;}

/**tab居左***/
.left_td table tr.last td{
    text-align: center;
    padding-top: 5px;
}

/*******系统管理*****/
.s-left {width:40%;}
.s-right { width:59%; margin-left:1%; display:inline;_margin-left:0.5%; }
ul.thead{ overflow:hidden;}
ul.thead li{ padding:0px; clear:both; display:block; height:165px;}
ul.thead li:hover{background:#f9f9f9;}
ul.thead li img{ padding:0 10px 0 0; display:block; vertical-align:bottom;}
ul.thead li a.txt{ display:block; width:220px; float:left;}
ul.thead a:hover{color: tomato}
.tb tbody tr td {line-height: 10px;}


/****图片水平垂直居中*****/
.wh50{ width:50px; height:50px;}
.wh50 img { width:auto !important; max-width:50px; width:50px; height:auto !important;max-height:50px;height:50px;}
.wh175{ width:175px; height:175px;}
.wh175 img { width:auto !important; max-width:175px; width:175px; height:auto !important;max-height:175px;height:175px;}
.wh70{ width:70px; height:70px;}
.wh70 img { width:auto !important; max-width:70px; width:70px; height:auto !important;max-height:70px;height:70px;}


.pic-wrap {display: table-cell;vertical-align: middle;text-align: center; }
.pic-wrap img { vertical-align: middle\9;/*由于ie中有默认高度的空文本节点*/ }

.notice_list{background: #fff; padding: 10px; height: 192px;}
.notice_list li{background: url("img/dot.gif") no-repeat 6px 12px; border-bottom: 0 none; height: 24px; line-height: 24px; overflow: hidden; padding-left: 13px;}

.clear_fix:after{
    clear: both;
    content: " ";
    display: block;
    height: 0;
    visibility: hidden;
}
.show_total h2{height: 40px;line-height: 40px;}
.total{width: 100%;background: #ffffff;}

.entrance h2 {font-size:14px;border:none;height: 30px;line-height: 30px;font-weight: 700;color: #455469;border-bottom: 0;padding: 0px;}
.side-m .item_total{background: #ffffff;padding: 10px;}
.side-m .item_total li{float: left;border:1px solid #ddd;margin-top:10px;margin-right:10px;width: 165px;height: 60px;}
.side-m .item_total li h3{height:20px;padding-top:5px;text-align: center;}
.side-m .item_total li h4{height:25px;padding-top:10px;font-size:16px;color: #0086C8;text-align: center;}
.side-m .item_chart{border-top:1px dotted #ddd;;}
