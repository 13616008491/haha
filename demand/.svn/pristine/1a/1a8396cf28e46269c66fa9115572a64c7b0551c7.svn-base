/*用户中心*/
.user_page{
	padding: 30px 0 90px 0;
	position: relative;
}
.user-l{
	width: 230px;
	padding: 20px;
	padding-left: 50px;
	background-color: #fff;
	position: relative;
}
.user-l label{
	font-size: 16px;
	color: #323232;
	font-weight: bold;
	position: relative;
}
.user-l label i{
	position: absolute;
	left: -30px;
	top:0px;
	font-weight: 100;
}
.user-l ul{
	padding: 26px 0;
}
.user-l ul:last-child{
	padding-bottom: 0
}
.user-l ul a{
	display: block;
	margin-bottom: 20px;
	cursor: pointer;
}
.user-l ul a:hover{
	color: #479ef8;
}
.user-l ul a:last-child{
	margin-bottom: 0
}
.user-l ul li.aft{
	color: #479ef8;
}
.user-l ul li{
	position: relative;
}
.user-l .aft-line{
	display: inline-block;
	height: 20px;
	width: 3px;
	background-color: #479ef8;
	position: absolute;
	left: -18px;
	top: -1px;
}
.user-r{
	width: 950px;
	min-height: 534px;
}
.user-det{
	background-color: #fff;
	margin-bottom: 20px;
	padding: 20px;
	text-align: center;
}
.user-det .imgCenterBox{
	width: 70px;
	height: 70px;
	line-height: 70px;
	border-radius: 35px;
	overflow: hidden;
	margin: 0 auto;
	margin-bottom: 20px;
}
.user-det i{
	color: #eee;
	margin-left: 10px;
}
.user-det i.is-aut{
	color: #fea802;
}
.user-det dt{
	font-size: 18px;
	color: #323232;
	margin-bottom: 4px;
}
.user-det dd{
	color: #999;
	margin-bottom: 20px;
}
.user-det .chhBtn{
	width: 100%;
	height: 36px;
	line-height: 36px;
	border-radius: 4px;
	background-color: #479ef8;
	color: #fff;
}
.user-nav{
	background-color: #fff;
	position: relative;
}
.user-nav li{
	height: 54px;
	line-height: 54px;
	color: #323232;
	padding: 0 20px;
	cursor: pointer;
}
.user-nav li.line{
	border-bottom: 1px solid #eee;
}
.user-nav li:hover{
	background-color: #479ef8;
	color: #fff;
}
.user-nav li.aft{
	color: #479ef8;
}
.user-nav li.aft:hover{
	color: #fff;
}
.user-nav i{
	display: block;
	height: 26px;
	width: 3px;
	background-color: #479ef8;
	position: absolute;
	left: 0;
	top: 14px;
}
.userpage{
	padding:0 30px;
	background-color: #fff;
}
.user-wrapbg{
	background-color: #fafafa;
}
.userpage .pages{
	border: none;
	padding: 30px 0;
}
.title{
	font-size: 16px;
	color: #323232;
	font-weight: bold;
	padding: 30px 30px 20px 30px;
	border-bottom: 1px solid #eee;
	background-color: #fff;
	position: relative;
}
.title i{
	float: right;
	font-size: 38px;
	color: #999;
	cursor: pointer;
	position: relative;
	top: -10px;
	font-weight: 400;
}
