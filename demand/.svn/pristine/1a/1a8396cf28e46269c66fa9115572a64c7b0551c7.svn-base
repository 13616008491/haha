/*招标状态*/
.tender-type{
	height: 70px;
	line-height: 70px;
}
.tender-type span{
	display: inline-block;
	height: 30px;
	width: 70px;
	border-radius: 4px;
	text-align: center;
	line-height: 30px;
	margin-right: 10px;
	cursor: pointer;
}

.tender-type span.aft{
	background-color: #479ef8;
	color: #fff !important;
}
.tender-list li{
	width: 20%;
	height: 100px;
	line-height: 100px;
	float: left;
	text-align: center;
	color: #323232;
}
.tender-list li i{
	color: #ff4e4e;
}
.tender-list ul{
	border-bottom: 1px solid #eee;
	cursor: pointer;
}
.tender-list a:last-child ul{
	border-bottom: 0px solid #eee;
}
.tender-list a ul:hover{
	background-color: #fafafa;
}
.tender-list-title li{
	height: 60px;
	line-height: 60px;
	background-color: #f5f5f5;
}
/*招标详情*/
.tender-info{
	border-bottom: 1px solid #eee;
	background-color: #fff;
}
.tender-info .tender-info-det label{
	display: block;
	color: #323232;
	margin-bottom: 10px;
}
.tender-info .tender-info-det p{
	color: #ff4e4e;
}
.tender-info .tender-info-det span{
	font-size: 30px;
}
.tender-info .tender-info-det{
	padding: 30px;
	padding-left: 70px;
	padding-right: 0;
	border-right: 1px solid #eee;
}
.tender-info .tender-info-time{
	padding: 30px;
	padding-left: 30px;
	padding-right: 0;
	position: relative;
	height: 129px;
}
.tender-info .tender-info-time p{
	color: #999;
}
.tender-info .tender-info-time p+p{
	color: #fa5d5d;
	position: absolute;
	bottom: 30px;
	left: 30px;
}
.tender-info .tender-info-time span{
	display: inline-block;
	height: 26px;
	width: 64px;
	text-align: center;
	line-height: 26px;
	border-radius: 4px;
	border: 1px solid #eee;
	position: absolute;
	right: 90px;
	top: 50%;
	margin-top: -13px;
}
/*投标中*/
.tender-info .tender-info-time span.font-blue{
	border: 1px solid #479ef8;
}
/*待开标*/
.tender-info .tender-info-time span.font-red{
	border: 1px solid #fa5d5d;
}
/*已流标*/
.tender-info .tender-info-time span.font-green{
	border: 1px solid #4eb168;
}
/*已开标、待投放*/
.tender-info .tender-info-time span.font_gray{
	border: 1px solid #999;
}
.tender-list-page .tender-list li{
	width: 25%;
}
.tender-list li .imgCenterBox{
	display: inline-block;
	height: 32px;
	width: 32px;
	line-height: 30px;
	border-radius:16px ;
	overflow: hidden;
	vertical-align: middle;
	line-height: 32px;
}
.appli-page .tender-list li .imgCenterBox{
	display: inline-block;
	height: 32px;
	width: 32px;
	line-height: 32px;
	border-radius:16px ;
	overflow: hidden;
	vertical-align: middle;
	line-height: 32px;
}

.tender-aut{
	line-height: 35px;
	height: 70px;
	color: #999;
	font-weight: bold;
	margin: 20px 0;
}
.tender-list-page .tender-list .brief-text{
	display: inline-block;
	line-height: 35px;
	margin-top: 15px;
	height: 70px;
	color: #323232;
	text-align: left;
	width: 150px;
}
.tender-aut div{
	text-align: left;
	padding-left: 10px;
	font-weight: 400;
}
.tender-aut span{
	padding: 2px 4px;
	border-radius: 4px;
	color: #999;
	border: 1px solid #999;
}
/*企业*/
.tender-aut span.font-blue{
	border: 1px solid #479ef8;
}
/*手机*/
.tender-aut span.font-red{
	border: 1px solid #fa5d5d;
}
/*保证金*/
.tender-aut span.font-green{
	border: 1px solid #4eb168;
}
/*实名*/
.tender-aut span.font-yellow{
	border: 1px solid #ffb15c;
}
.tender-list .chhBtn{
	height: 28px;
	line-height: 28px;
	width: 60px;
	text-align: center;
	background-color: #479ef8;
	color: #fff;
}
